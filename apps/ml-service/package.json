{
  "name": "@aladdin/ml-service",
  "version": "1.0.0",
  "description": "Production ML service for cryptocurrency price prediction using PyTorch",
  "private": true,
  "scripts": {
    "dev": "venv/bin/uvicorn src.main:app --reload --port 8000",
    "start": "venv/bin/uvicorn src.main:app --host 0.0.0.0 --port 8000",
    "start:prod": "venv/bin/uvicorn src.main:app --host 0.0.0.0 --port 8000 --workers 4",
    "test": "venv/bin/python test_ml.py",
    "format": "venv/bin/black src/ && venv/bin/ruff check src/ --fix",
    "lint": "venv/bin/ruff check src/",
    "setup": "python3.11 -m venv venv && venv/bin/pip install -r requirements.txt"
  },
  "keywords": [
    "machine-learning",
    "pytorch",
    "lstm",
    "cryptocurrency",
    "prediction"
  ],
  "author": "Aladdin Team"
}
